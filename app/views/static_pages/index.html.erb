<section class="todoapp">
  <header class="header">
    <h1>Tasks to Complete</h1>
  </header>

  <section class="main">
    <!-- Add htmlString + liElement vars within todo-list -->
    <ul class="todo-list">


    </ul>
  </section>
</section>
<!-- $(fuction() {CODE HERE}); loads JS code after page has loaded -->
<script>
  $(function() { 
    $.get("/tasks").success (function( data ) {
      var htmlString = "";

      $.each(data, function(index, task) {
        var checkedStatus = task.done ? "checked" : "";
        var liElement = '<li><div class="view"><input class="toggle" type="checkbox"' + checkedStatus + '><label>' + task.title + '</label></div></li>';
        htmlString += liElement;
      });
      var ulTodos = $('.todo-list');
      ulTodos.html(htmlString);

    });
  });
</script>